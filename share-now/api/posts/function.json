{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "posts/{id:int?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "direction": "in",
      "name": "teamsfxContext",
      "type": "TeamsFx"
    }
  ],
  "scriptFile": "../dist/posts/index.js"
}